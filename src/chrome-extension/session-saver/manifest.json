{
  "name": "Jira Cookie Sender",
  "description": "Send cookies to Jira",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "cookies",
    "storage",
    "webNavigation",
    "scripting"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}
